<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>QCM-ENI-10A</display-name>
  <welcome-file-list>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AccesStagiaires</servlet-name>
    <jsp-file>/login/AccesStagiaires.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccesStagiaires</servlet-name>
    <url-pattern>/login/AccesStagiaires</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ValiderAcces</display-name>
    <servlet-name>ValiderAcces</servlet-name>
    <servlet-class>fr.qcm_eni_10A.servlets.ValiderAcces</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValiderAcces</servlet-name>
    <url-pattern>/login/validerAcces</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>AccueilStagiaire</display-name>
    <servlet-name>AccueilStagiaire</servlet-name>
    <jsp-file>/Stagiaire/AccueilStagiaire.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccueilStagiaire</servlet-name>
    <url-pattern>/stagiaire/AccueilStagiaire</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>Pool de connexions vers la base QCM</description>
    <res-ref-name>jdbc/Qcm</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <description></description>
    <display-name>ListerQuestions</display-name>
    <servlet-name>ListerQuestions</servlet-name>
    <servlet-class>fr.qcm_eni_10A.servlets.ListerQuestions</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListerQuestions</servlet-name>
    <url-pattern>/ListerQuestions</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>Qcm</servlet-name>
    <jsp-file>/Stagiaire/Qcm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Qcm</servlet-name>
    <url-pattern>/Qcm</url-pattern>
  </servlet-mapping>
  
</web-app>